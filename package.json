{
  "name": "mute-bot-storage",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "server.js",
    "start:dev": "npm run build:live",
    "build": "webpack",
    "build:live": "nodemon --exec ts-node ./src/index.ts | bunyan",
    "protoc": "protoc/protoc-3.2.0-linux-x86_64 --js_out=import_style=commonjs,binary:. src/message.proto",
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg"
  },
  "bin": {
    "mute-bot-storage": "server.js"
  },
  "keywords": [
    "bot",
    "storage",
    "peer-network"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.14.0",
    "mongoose": "^4.7.1",
    "mute-structs": "^0.1.2",
    "netflux": "latest",
    "uws": "^0.12.0"
  },
  "devDependencies": {
    "@types/bunyan": "0.0.35",
    "@types/express": "^4.0.34",
    "@types/mongoose": "^4.6.2",
    "@types/node": "^7.0.5",
    "bunyan": "^1.8.5",
    "commitizen": "^2.9.5",
    "cz-conventional-changelog": "^1.2.0",
    "google-protobuf": "~3.2.0",
    "husky": "^0.13.1",
    "nodemon": "^1.11.0",
    "ts-loader": "^2.0.0",
    "ts-node": "^2.0.0",
    "tslint": "^4.0.2",
    "typescript": "^2.0.10",
    "validate-commit-msg": "^2.11.1",
    "webpack": "^2.2.1",
    "webpack-node-externals": "^1.5.4"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
