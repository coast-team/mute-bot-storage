{
  "name": "mute-bot-storage",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "npm run build:live",
    "build": "webpack",
    "build:live": "nodemon --exec ts-node ./src/index.ts -- --logLevel trace | bunyan",
    "lint": "tslint src/*.ts src/**/*.ts -e 'src/**/*.proto' -e 'src/**/*.d.ts'",
    "proto": "pbjs -t static-module -w es6 --no-verify --no-delimited --no-convert src/proto/index.proto -o src/proto/index.es6.js && pbts src/proto/index.es6.js -o src/proto/index.d.ts",
    "postproto": "babel --presets env -o src/proto/index.js src/proto/index.es6.js && rm src/proto/index.es6.js",
    "cz": "git-cz",
    "commitmsg": "validate-commit-msg"
  },
  "bin": {
    "mute-bot-storage": "server.js"
  },
  "keywords": [
    "bot",
    "storage",
    "peer-network"
  ],
  "author": "",
  "license": "AGPL-3.0",
  "dependencies": {
    "bunyan": "^1.8.8",
    "kcors": "^2.2.1",
    "koa": "^2.2.0",
    "koa-router": "^7.2.1",
    "mongoose": "^4.7.1",
    "mute-core": "^0.1.2",
    "mute-structs": "^0.1.2",
    "netflux": "latest",
    "rxjs": "^5.1.0",
    "uws": "^0.14.1"
  },
  "devDependencies": {
    "@types/bunyan": "1.8.0",
    "@types/commander": "^2.3.31",
    "@types/express": "^4.0.34",
    "@types/mongoose": "^4.6.2",
    "@types/node": "^7.0.5",
    "commander": "^2.9.0",
    "commitizen": "^2.9.5",
    "cz-conventional-changelog": "^2.0.0",
    "husky": "^0.13.1",
    "nodemon": "^1.11.0",
    "ts-loader": "^2.0.0",
    "ts-node": "^3.0.2",
    "tslint": "^5.1.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "~2.3.4",
    "validate-commit-msg": "^2.11.1",
    "webpack": "^3.0.0",
    "webpack-node-externals": "^1.5.4"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
