{
  "name": "mute-bot-storage",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "npm run build:live",
    "build": "webpack",
    "build:live": "nodemon --exec ts-node ./src/index.ts -- --cors --logLevel trace | bunyan",
    "builddev": "npm run build && scp server.js ovhmute:/root/container/botStorage",
    "lint": "tslint src/*.ts src/**/*.ts -e 'src/**/*.proto' -e 'src/**/*.d.ts' -p tsconfig.json",
    "proto": "pbjs -t static-module -w es6 --no-verify --no-delimited --no-convert src/proto/index.proto -o src/proto/index.es6.js && pbts src/proto/index.es6.js -o src/proto/index.d.ts",
    "postproto": "babel --presets env -o src/proto/index.js src/proto/index.es6.js && rm src/proto/index.es6.js",
    "cz": "git-cz",
    "precommit": "npm run lint",
    "commitmsg": "validate-commit-msg",
    "semantic-release": "semantic-release --no-ci"
  },
  "files": [
    "dist/server.js"
  ],
  "keywords": [
    "bot",
    "storage",
    "peer-network"
  ],
  "author": "",
  "engines": {
    "node": ">=8.1.4"
  },
  "license": "AGPL-3.0",
  "dependencies": {
    "bunyan": "^1.8.12",
    "commander": "^2.15.1",
    "crypto-api-wrapper": "^0.4.1-beta",
    "kcors": "^2.2.1",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "longjohn": "^0.2.12",
    "mongoose": "^5.1.2",
    "mute-core": "^1.0.3",
    "netflux": "^4.0.5",
    "node-webcrypto-ossl": "^1.0.37",
    "rxjs": "^6.2.0",
    "uws": "^10.148.0"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.4",
    "@types/commander": "^2.12.2",
    "@types/express": "^4.11.1",
    "@types/kcors": "^2.2.3",
    "@types/koa": "^2.0.45",
    "@types/koa-bodyparser": "^4.2.0",
    "@types/koa-router": "^7.0.28",
    "@types/mongoose": "^5.0.14",
    "@types/node": "^8.10.17",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "commitizen": "^2.10.1",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^0.14.3",
    "nodemon": "^1.17.5",
    "prettier": "^1.12.1",
    "semantic-release": "^15.5.0",
    "ts-loader": "^4.3.0",
    "ts-node": "^6.0.5",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-eslint-rules": "^5.3.1",
    "typescript": "~2.7.2",
    "validate-commit-msg": "^2.11.1",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.4",
    "webpack-node-externals": "^1.7.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
