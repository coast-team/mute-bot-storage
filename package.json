{
  "name": "mute-bot-storage",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "npm run build:live",
    "build": "webpack",
    "build:live": "nodemon --exec ts-node ./src/index.ts -- --logLevel trace | bunyan",
    "builddev": "npm run build && scp server.js ovhmute:/root/container/botStorage",
    "lint": "tslint src/*.ts src/**/*.ts -e 'src/**/*.proto' -e 'src/**/*.d.ts' -p tsconfig.json",
    "proto": "pbjs -t static-module -w es6 --no-verify --no-delimited --no-convert src/proto/index.proto -o src/proto/index.es6.js && pbts src/proto/index.es6.js -o src/proto/index.d.ts",
    "postproto": "babel --presets env -o src/proto/index.js src/proto/index.es6.js && rm src/proto/index.es6.js",
    "cz": "git-cz",
    "precommit": "npm run lint",
    "commitmsg": "validate-commit-msg"
  },
  "files": [
    "server.js"
  ],
  "keywords": [
    "bot",
    "storage",
    "peer-network"
  ],
  "author": "",
  "engines": {
    "node": ">=8.1.4"
  },
  "license": "AGPL-3.0",
  "dependencies": {
    "bunyan": "^1.8.12",
    "commander": "^2.12.1",
    "kcors": "^2.2.1",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-router": "^7.3.0",
    "mongoose": "^4.13.4",
    "mute-core": "^0.3.5",
    "netflux": "^2.0.3",
    "rxjs": "^5.5.2",
    "uws": "^8.14.1"
  },
  "devDependencies": {
    "@types/commander": "^2.11.0",
    "@types/express": "^4.0.39",
    "@types/koa": "^2.0.41",
    "@types/koa-router": "^7.0.26",
    "@types/mongoose": "^4.7.28",
    "@types/node": "^8.0.53",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^0.14.3",
    "nodemon": "^1.11.0",
    "ts-loader": "^3.1.1",
    "ts-node": "^3.0.2",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "~2.5.3",
    "validate-commit-msg": "^2.11.1",
    "webpack": "^3.8.1",
    "webpack-node-externals": "^1.5.4"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
